PImage stickmen;

String message1 =  "I'm pretty sure there are men living on the surface of your eyes";
String message2 = "I can see them pairing up";
String message3 = "fighting, maybe, or else dancing.";

PFont f;

void setup(){
  size(626,438);
  background(0, 10);
  noFill();
  stickmen = loadImage("stick men.png");
  f = createFont("OpenDyslexic",15,true);  
}

void draw() { 
  strokeWeight(random(10, 20));
  stroke(random(0,50), random(0,75), random(0,100));
  float rainbow_size = random(200, 270);
  ellipse(416, 219, rainbow_size, rainbow_size);
  fill(0);
  ellipse(416, 219, 50, 50);
  
  strokeWeight(random(10, 20));
  stroke(random(0,50), random(0,75), random(0,100));
  ellipse(208, 219, rainbow_size, rainbow_size);
  fill(0);
  ellipse(208, 219, 50, 50);
  
  image(stickmen, 250, 150);
  
  fill(75, 100, 150);
  textFont(f);                    
  textAlign(LEFT);            
  text(message1,50,40);
  text(message2,10,400);
  text(message3,355,400);
  
  }
